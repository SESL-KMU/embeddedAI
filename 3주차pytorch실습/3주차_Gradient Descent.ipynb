      "source": [
        "import torch\n",
        "\n",
        "# 변수 선언\n",
        "W = torch.randn(1, requires_grad=True)\n",
        "\n",
        "# 간단한 경사하강법\n",
        "learning_rate = 0.1\n",
        "for i in range(100):\n",
        "    cost = (W - 3) ** 2\n",
        "    cost.backward()\n",
        "\n",
        "    with torch.no_grad():\n",
        "        W -= learning_rate * W.grad\n",
        "        W.grad.zero_()\n",
        "\n",
        "    print(W.item())\n"
      ]
    }
